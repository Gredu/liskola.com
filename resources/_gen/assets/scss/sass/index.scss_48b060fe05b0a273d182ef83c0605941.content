@font-face{font-family:TravelingTypewriter;src:url(/font/TravelingTypewriter.ttf),url(/font/TravelingTypewriter.otf)}body{background-color:#000;color:#fff;margin:0;padding:0;background-image:linear-gradient(rgba(0,0,0,0.7),rgba(0,0,0,0.7)),url("/greenarrow-parkinglot.jpg");background-size:cover;background-position-y:-120px;font-family:TravelingTypewriter,sans-serif;font-size:16px}a{color:#00adef;font-weight:none;text-decoration:none}a:link,a:visited{color:#00adef;font-weight:700}.max-width{max-width:950px;margin:2em auto}.container{display:grid;padding:0 1em;grid-template-rows:auto auto 400px;grid-template-areas:"header profile" "main profile" "footer footer"}@media only screen and (max-width:424px){.container{grid-template-areas:"header header" "main main" "footer footer"}}.header{grid-area:header}.header img{width:350px;height:auto}.main{grid-area:main;align-items:center}.main em{display:block;margin:0 2em}.main img{width:280px;margin:1.2em auto;display:none}@media only screen and (max-width:424px){.main img{display:block}}.footer{grid-area:footer;margin:0 auto;padding-top:3em}.profile{grid-area:profile}.profile img{margin:20px 20px 4px;max-width:280px;border:solid 2px #fff;display:none}@media only screen and (min-width:425px){.profile img{display:block}}.profile ul{display:none;margin-top:0;padding-left:26px;text-align:center;width:280px}@media only screen and (min-width:425px){.profile ul{display:block}}.profile ul li{display:inline-block;font-size:20px;margin-right:30px}.profile iframe{border:solid 2px #fff}